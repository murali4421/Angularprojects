@model IEnumerable<EcomCore.CandidateList>

@{
    ViewData["Title"] = "Candidates";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3>Candidates List</h3>

<p>
    <a asp-action="Add">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Party_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MP_State)
            </th>
           
            <th>
                @Html.DisplayNameFor(model => model.MP_Seat)
            </th>
          
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Party_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MP_State)
            </td>
           
            <td>
                @Html.DisplayFor(modelItem => item.MP_Seat)
            </td>
           
            <td>
                @Html.ActionLink("Edit", "Edit", new {  id= item.Id  }) |
                    @Html.ActionLink("Delete", "Delete", new { id=item.Id })
            </td>
        </tr>
}
    </tbody>
</table>

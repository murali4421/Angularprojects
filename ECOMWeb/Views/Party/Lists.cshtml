@model IEnumerable<EcomCore.PartyLists>

@{
    ViewData["Title"] = "Lists";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3>Parties</h3>

<p>
    <a asp-action="Add">New Parties</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Symbol)
            </th>
            @*<th>
                @Html.DisplayNameFor(model => model.Id)
            </th>*@
            <th>
                @Html.DisplayNameFor(model => model.Party_Name)
            </th>
           @* <th>
                @Html.DisplayNameFor(model => model.Symbol_Id)
            </th>*@
            <th>
                @Html.DisplayNameFor(model => model.Register_Date)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
                <td>
                    <img src="/Symbol/RetrieveImage/@item.Symbol_Id" alt="" height=30 width=30 />
                </td>
           @* <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>*@
            <td>
                @Html.DisplayFor(modelItem => item.Party_Name)
            </td>
            @*<td>
                @Html.DisplayFor(modelItem => item.Symbol_Id)
            </td>*@
            <td>
                @Html.DisplayFor(modelItem => item.Register_Date)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new {  id=item.Id  }) |
                @Html.ActionLink("Delete", "Delete", new {  id=item.Id  })
            </td>
        </tr>
}
    </tbody>
</table>

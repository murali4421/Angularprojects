@model IEnumerable<EcomCore.Symbols>

@{
    ViewData["Title"] = "Symbols ";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h4>Symbols</h4>
<hr />
<p>
    <a asp-action="Add">New Symbol</a>
</p>
<table class="table">
    <thead>
        <tr>           
           <th>
                @Html.DisplayNameFor(model => model.Symbol)
            </th>
            <th>
                @Html.Label("Symbol Name")
            </th>
            <th>
                @Html.Label("Action")
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
               <img src="/Symbol/RetrieveImage/@item.Id" alt="" height=30 width=30 />
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Symbol_Name)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
               @* @Html.ActionLink("Details", "Details", new { id=item.Id }) |*@
                    @Html.ActionLink("Delete", "Delete", new { id=item.Id })
            </td>
       </tr>
            }
    </tbody>
</table>
